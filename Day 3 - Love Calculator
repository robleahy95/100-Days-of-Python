print("The Love Calculator is calculating your score...")
name1 = input() # What is your name?
name2 = input() # What is their name?
# ðŸš¨ Don't change the code above ðŸ‘†
# Write your code below this line ðŸ‘‡
full_name = name1 + name2
lower_names = full_name.lower()

t = lower_names.count("t")
r = lower_names.count("r")
u = lower_names.count("u")
e = lower_names.count("e")
first_digit = t + r + u + e

l = lower_names.count("l")
o = lower_names.count("o")
v = lower_names.count("v")
e = lower_names.count("e")
second_digit = l + o + v + e

result = str(first_digit) + str(second_digit)
result_new = int(result)

if result_new < 10 or result_new > 90:
  print(f"Your score is {result_new}, you go together like coke and mentos.")
elif result_new >= 40 and result_new <= 50:
  print(f"Your score is {result_new}, you are alright together.")
else:
  print(f"Your score is {result_new}.")
